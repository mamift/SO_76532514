<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" >
	<xs:element name="AdministrativeContactInformation">
		<xs:annotation>
			<xs:documentation>Used to define adminstrative contact information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Name"/>
				<xs:element ref="Address" minOccurs="0"/>
				<xs:element ref="eMail" minOccurs="0"/>
				<xs:element ref="PhoneNumber" minOccurs="0"/>
				<xs:element ref="FaxNumber" minOccurs="0"/>
				<xs:element ref="FreeTextField" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AllocationCompany" type="CompanyCode">
		<xs:annotation>
			<xs:documentation>Name of company who is responsible for allocation and maintenance of codes</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AssociatedAttachedTrainID" type="CompositIdentifierPlannedType">
		<xs:annotation>
			<xs:documentation>TrainID of the Associated Train in an Attach Activity</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AssociatedAttachedOTN" type="String1-8">
		<xs:annotation>
			<xs:documentation>Identifies the associated train for the activity for traffic management purposes by the Dispatcher, GSMR services, etc.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BrakeType">
		<xs:annotation>
			<xs:documentation>Type of braking system</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:enumeration value="G"/>
				<xs:enumeration value="P"/>
				<xs:enumeration value="R"/>
				<xs:enumeration value="M"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="BrakeWeight">
		<xs:annotation>
			<xs:documentation>Shows the Braked mass of the wagon according to the type of the braking system, in Tonnes</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:minInclusive value="1"/>
				<xs:maxInclusive value="99999"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="BrakingRatio">
		<xs:annotation>
			<xs:documentation>Minimum percentage of braking. Expressed as an integer value (no percent sign should be added).</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:integer">
				<xs:maxInclusive value="999"/>
				<xs:minInclusive value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="BookedLocationDateTime" type="xs:dateTime">
		<xs:annotation>
			<xs:documentation>Scheduled Date and Time of a train at a speciied location as defined in the path contract</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TypeOfInformation">
		<xs:annotation>
			<xs:documentation>Enumeration indicating to which process step / process type in the planning does the message belong: path study; pre-arranged/catalogue path; (draft) offer; final offer; booked; deleted; utilisation notification; confirmation of utilisation confirmation
                        PathStudy = 1,
                        CataloguePath = 2,
                        DraftOffer = 3,
                        FinalOffer = 4,
                        Booked = 5,
                        Deleted = 6,
                        UtilisationNotification = 7,
                        UtilisationConfirmation = 8
			</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:integer">
				<xs:minInclusive value="1"/>
				<xs:maxInclusive value="8"/>
				<xs:enumeration value="1"/>
				<xs:enumeration value="2"/>
				<xs:enumeration value="3"/>
				<xs:enumeration value="4"/>
				<xs:enumeration value="5"/>
				<xs:enumeration value="6"/>
				<xs:enumeration value="7"/>
				<xs:enumeration value="8"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Recipient">
		<xs:annotation>
			<xs:documentation>Receiver of the message</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="CompanyCode">
					<xs:attribute ref="CI_InstanceNumber"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
  
	<xs:element name="CFRTrainInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Indicativ" type="xs:long" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Internal Train ID from CFR reporting applications</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TrainHeader" type="xs:long" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Train header ID from CFR train programmging applications (AtlasIM)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="VitezaMaxima" type="xs:int" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The maximum speed of the train in km/h</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ProcentFranare" type="xs:int" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The required percent of braking mass of the train</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MersTren" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Timetable year format yyyy</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DataProgramata" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Date and time of scheduled departure. Format yyyy-MM-ddTHH:mm:ss</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Mentiuni" minOccurs="0" />
				<xs:element ref="Observatii" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
