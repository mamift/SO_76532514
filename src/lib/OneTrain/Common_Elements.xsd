  <xs:element name="TranzitInternational" >
    <xs:annotation>
      <xs:documentation>
        Specifies if the train is transit on the CFR network railways
      </xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:unsignedByte">
        <xs:enumeration value="0">
          <xs:annotation>
            <xs:documentation>The train does not transit CFR network</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="1">
          <xs:annotation>
            <xs:documentation>The train transit CFR network. 
            Is an international train with origin and destination stations outside CFR.
            It's used to select if can benefit of some discount in fees.
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="International" >
    <xs:annotation>
      <xs:documentation>
        Specifies if the train exit CFR network and cross an CFR international border
      </xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:unsignedByte">
        <xs:enumeration value="0">
          <xs:annotation>
            <xs:documentation>The train does not exit CFR network</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="1">
          <xs:annotation>
            <xs:documentation>
              The train exits CFR network. The destination station is outside CFR.
              It's used to select if the train will be added in other applications (ie. IMComm - messages at CFR-MAV border).
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

</xs:schema>
